{
  "name": "toon-utils",
  "version": "1.1.0",
  "description": "TypeScript utilities for parsing, encoding, and validating TOON (Token-Oriented Object Notation) format",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "bin": {
    "toon-parser": "./dist/cjs/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./min": {
      "import": "./dist/esm/index.min.js",
      "require": "./dist/cjs/index.min.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**/!(*.test.*|__tests__)",
    "dist/**/*.d.ts",
    "dist/**/*.js",
    "dist/**/*.map",
    "!dist/**/__tests__",
    "!dist/**/*.test.*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run minify",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json && node scripts/fix-esm.js",
    "build:umd": "rollup -c rollup.config.js",
    "minify": "node scripts/minify.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "keywords": [
    "toon",
    "parser",
    "encoder",
    "serialization",
    "json",
    "llm",
    "token-oriented",
    "typescript"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "rollup": "^4.53.3",
    "terser": "^5.44.1",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
